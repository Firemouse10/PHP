<?php 
include("conexao.php");


$sql1 = "INSERT INTO dados (nome, endereco, cidade, estado) VALUES ('$nome', '$endereco', '$cidade', '$estado')";
            $resultado = mysqli_query($c, $sql1);

            if (!$resultado) {
                echo "Erro ao registrar dados: " . mysqli_error($c);
            } else {
                echo "Dados registrados com sucesso!";
            }
